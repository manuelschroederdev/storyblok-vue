{
  "compilerOptions": {
    "module": "ESNext",
    "isolatedModules": true,
    "target": "ESNext",
    "strict": false,
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": true
  },
  "extends": "@vue/tsconfig/tsconfig.json",
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue",
  "include": ["./*.vue", "./**/*.ts"],
  "exclude": ["node_modules/*"]
}